{
  "manifest_version": 2,

  "name": "API inspector",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.baidu.com/*"
  ],
  "background": {
    "scripts": [
        "background.js"
    ]
  }
}
